            </li>
          </ul>
        </div>
      </div>
    </nav>
    
    <!-- Page Content -->
    <div class="container">
        <img class="imagemBanner" src="../img/Banner.jpg">
        
     

      
  {% block conteudoCentral %}
       <!-- Page Heading -->
      <h1 class="tituloSite">Bem-Vindo ao 
        <small>HoradeMudar.com</small>
      </h1>
      <div class="row">
          {% for imoveis in imoveis  %}
         
    
        <div class="col-lg-4 col-sm-6 portfolio-item">
             {% if imoveis.imagem1 %}<a href="../{{imoveis.imagem1}}" data-lightbox="{{imoveis.imagem1}}" data-title="My caption"><img class="card-img-top"  src="../{{imoveis.imagem1}}"></a>  {% endif %}
             {% if imoveis.imagem2 %}<a href="../{{imoveis.imagem2}}" data-lightbox="{{imoveis.imagem2}}" data-title="My caption"><img class="card-img-top"  src="../{{imoveis.imagem2}}"></a>  {% endif %}
             {% if imoveis.imagem3 %}<a href="../{{imoveis.imagem3}}" data-lightbox="{{imoveis.imagem3}}" data-title="My caption"><img class="card-img-top"  src="../{{imoveis.imagem3}}"></a>  {% endif %}
             {% if imoveis.imagem4 %}<a href="../{{imoveis.imagem4}}" data-lightbox="{{imoveis.imagem4}}" data-title="My caption"><img class="card-img-top"  src="../{{imoveis.imagem4}}"></a>  {% endif %}
             {% if imoveis.imagem5 %}<a href="../{{imoveis.imagem5}}" data-lightbox="{{imoveis.imagem5}}" data-title="My caption"><img class="card-img-top"  src="../{{imoveis.imagem5}}"></a>  {% endif %}
            <div class="card-body">
              <h4 class="card-title">
                 {{imoveis.titulo}}
              </h4>
              <p class="card-text">{{imoveis.descricao}}</p>
           
       {%endfor%}
    {% endblock %}
      <!-- /.row -->
    </div>
    <!-- /.container -->

    <!-- Footer -->
    <footer class="py-5 bg-dark">
      <div class="container">
        <p class="m-0 text-center text-white">Copyright &copy; HoraDeMudar.com 2018</p>
      </div>
      <!-- /.container -->
    </footer>
{% block rodape %}
                        <script src="js/jquery-3.3.1.min.js" type="text/javascript" ></script>
